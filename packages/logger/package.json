{
  "name": "@tkottke90/logger",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@tkottke-js-helpers/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/logger/dist",
          "main": "packages/logger/src/index.ts",
          "tsConfig": "packages/logger/tsconfig.lib.json",
          "format": [
            "esm"
          ],
          "declarationRootDir": "packages/logger/src"
        }
      }
    }
  },
  "dependencies": {
    "vitest": "*",
    "winston": "*",
    "winston-loki": "*",
    "@tkottke90/js-errors": "*"
  }
}
